{
  "name": "jest-codemods",
  "version": "0.19.1",
  "description": "Codemods for migrating test files to Jest",
  "license": "MIT",
  "repository": "skovhus/jest-codemods",
  "author": {
    "name": "Kenneth Skovhus",
    "email": "kenneth.skovhus@gmail.com",
    "url": "https://github.com/skovhus"
  },
  "bin": "bin/jest-codemods.js",
  "engines": {
    "node": ">=6"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "babel src -d dist --ignore='src/**/*.test.js'",
    "check": "npm run lint && npm run test:cov",
    "check:bail": "eslint src && npm run test:cov",
    "clean": "rm -rf lib coverage npm-debug.log dist",
    "lint": "eslint --fix src",
    "format": "prettier --ignore-path .gitignore --write './**/*.js'",
    "precommit": "lint-staged",
    "prepare": "npm run build",
    "prepublish": "npm run clean && npm run check:bail",
    "ci": "npm run check:bail && codecov",
    "test:cov": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "test": "jest --runInBand"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "keywords": [
    "codemod",
    "jest",
    "ava",
    "tape",
    "mocha",
    "chai",
    "proxyquire",
    "migrate",
    "migrations",
    "upgrade",
    "transform",
    "jscodeshift",
    "codemods"
  ],
  "dependencies": {
    "chalk": "~1.1.3",
    "execa": "~0.4.0",
    "globby": "^8.0.1",
    "inquirer": "^6.2.1",
    "is-git-clean": "~1.1.0",
    "jscodeshift": "^0.6.2",
    "meow": "~3.7.0",
    "update-notifier": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.6",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "codecov": "^3.1.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.3.1",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "lint-staged": "^4.2.3",
    "prettier": "^1.8.2"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0",
    "sane": "^3.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "testEnvironment": "node"
  }
}
